{
  "compilerOptions": {
    "module": "es6",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "lib": ["ESNext"],
    "strictNullChecks": false,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "compileOnSave": true,
  "include": ["src"]
}
